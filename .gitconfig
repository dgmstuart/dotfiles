[core]
	excludesfile = ~/.gitignore_global
        whitespace = trailing-space
[push]
	default = simple
[pull]
	rebase = true
	ff = only
[merge]
	ff = only
[color]
	status = auto
	branch = auto
[color "diff"]
	plain = normal
	meta = yellow
	frag = blue
	old = red
	new = green
	commit = yellow
	whitespace = normal red
