# zsh
alias ohmyzsh="subl ~/.oh-my-zsh"
alias zshplugins='ll ~/.oh-my-zsh/plugins'
alias zshconfig="vim ~/.zshrc"
alias szsh='source ~/.zshrc'

# Editors
alias -g subl=subl
alias v='vim'
alias mvm='mvim'

# Listing
alias l='ls -F'
alias ll='ls -alo'
alias lt='ls -alort'
alias lh='ls -alh'

# Projects
alias soldn='cd ~/Sites/swingoutlondon'
alias ssoldn='soldn; subl .'

# Git
alias gce='git config -e'
